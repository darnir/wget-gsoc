<WgetTest name="Test HEAD Requests">
<!--
     Ensure that Wget does not download ANY files when HEAD Requests are sent
-->

    <InputFile name="File2.html"><![CDATA[<html>
<head>
</head>
<body>
2nd Test File
</body>
</html>]]></InputFile>

    <Option>-d</Option>

    <Special command="Header">
        <Name>Content-Length</Name>
        <Value>44</Value>
    </Special>

    <ExpectedResults>
        <ReturnCode>0</ReturnCode>
    </ExpectedResults>

</WgetTest>
