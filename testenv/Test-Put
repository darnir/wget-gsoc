<WgetTest name="Test PUT Command">
<!--
     Test sending a HTTP PUT Request to a non-existant URI
-->

    <Option>-d --method=put --body-data="Frodo Baggins!"</Option>

    <ServerRule command="PUT">
        <File>newfile</File>
    </ServerRule>

    <ExpectedResults>
        <ReturnCode>0</ReturnCode>
        <File>newfile</File>
    </ExpectedResults>

</WgetTest>
