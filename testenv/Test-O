<WgetTest name="Test -O Option">
<!--
     Ensure that Wget writes to the correct filename with -O
-->

    <InputFile name="Sparta">This is Spartaa!</InputFile>

    <Option>-O Persia</Option>

    <ExpectedResults>
        <ReturnCode>0</ReturnCode>
        <File orig="Sparta">Persia</File>
    </ExpectedResults>

</WgetTest>
