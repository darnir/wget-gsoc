<WgetTest name="Test -c Partial">
<!--
     Test Wget's actions with a partially downloaded file.
-->

    <InputFile name="File1.html"><![CDATA[<html>
<head>
</head>
<body>
This is a Prototype File.
</body>
</html>]]></InputFile>

    <Option>-d -c</Option>
    <ServerRule command="Continue">
        <File>File1.html</File>
        <Bytes>18</Bytes>
    </ServerRule>

    <ExpectedResults>
        <ReturnCode>0</ReturnCode>
        <File>File1.html</File>
    </ExpectedResults>

</WgetTest>
