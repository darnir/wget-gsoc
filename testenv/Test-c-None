<WgetTest name="Test -c None">
<!--
     Test Wget with continue option when requested file is not preset
     on disk at all.
-->

    <InputFile name="File1.html"><![CDATA[<html>
<head>
</head>
<body>
This is a Prototype File.
</body>
</html>]]></InputFile>

    <Option>-d -c</Option>
    <Special command="Continue">
        <File>File1.html</File>
    </Special>

    <ExpectedResults>
        <ReturnCode>0</ReturnCode>
        <File>File1.html</File>
    </ExpectedResults>

</WgetTest>
