<WgetTest name="Prototype File">

    <InputFile name="File1.html" download="False"><![CDATA[<html>
<head>
</head>
<body>
This is a Prototype File.
</body>
</html>]]></InputFile>

    <InputFile name="File2.html"><![CDATA[<html>
<head>
</head>
<body>
2nd Test File
</body>
</html>]]></InputFile>

    <Option>-d -c</Option>
    <Option>-v</Option>

    <Special command="Redirect">
        <From>File2.html</From>
        <To>temp</To>
        <Code>301</Code>
    </Special>
    <Special command="Redirect">
        <From>temp</From>
        <To>File1.html</To>
        <Code>302</Code>
    </Special>

    <ExpectedResults>
        <ReturnCode>0</ReturnCode>
        <File orig="File1.html">File2.html</File>
    </ExpectedResults>

</WgetTest>
